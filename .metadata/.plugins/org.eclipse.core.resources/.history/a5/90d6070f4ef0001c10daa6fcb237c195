package objects;

import javax.swing.ImageIcon;
import javax.swing.JLabel;

public class Disk extends JLabel implements Object{
	public Disk(ImageIcon img) {
		super(img);
	}

	@Override
	public void shape(Disk baseDisk) {
		this.setBounds(baseDisk.getX()+19, baseDisk.getY()-baseDisk.getHeight()+10, baseDisk.getWidth()-38, baseDisk.getHeight()-5);
	}
}
